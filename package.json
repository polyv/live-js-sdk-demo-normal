{
  "name": "live-js-sdk-demo-normal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "serve:ie": "cross-env VUE_APP_BROWSER=IE npm run serve",
    "dev": "npm run lint && vue-cli-service serve",
    "dev:ie": "cross-env VUE_APP_BROWSER=IE npm run dev",
    "build": "vue-cli-service build",
    "lint:es": "eslint --ext .js,vue .",
    "lint:style": "stylelint **/*.{css,scss,sass,vue}",
    "lint": "npm run lint:es && npm run lint:style"
  },
  "dependencies": {
    "@polyv/interactions-receive-sdk": "0.11.2",
    "@polyv/interactions-receive-sdk-ui-default": "0.11.2",
    "axios": "^0.27.2",
    "core-js": "^3.6.5",
    "jraiser": "^3.1.5",
    "spark-md5": "^3.0.2",
    "vant": "^2.12.50",
    "vue": "2.6.11",
    "vue-router": "^3.2.0",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@polyv/eslint-config": "0.3.0",
    "@vue/cli-plugin-babel": "~4.5.8",
    "@vue/cli-plugin-router": "~4.5.8",
    "@vue/cli-plugin-vuex": "~4.5.8",
    "@vue/cli-service": "~4.5.8",
    "babel-plugin-import": "^1.13.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.17.0",
    "lint-staged": "^9.5.0",
    "node-sass": "^5.0.0",
    "sass-loader": "^10.0.5",
    "sockjs-client": "1.5.1",
    "stylelint": "^13.7.2",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.18.0",
    "vue-template-compiler": "2.6.11",
    "vue-awesome-swiper": "4.1.1",
    "yorkie": "2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "npm run lint:es",
      "git add"
    ],
    "*.{css,sass,scss,vue}": [
      "npm run lint:style",
      "git add"
    ]
  }
}
